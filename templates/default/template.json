{
    "name": "Default Dashboard",
    "description": "Modern, responsive dashboard with glassmorphism design and Alpine.js interactivity",
    "version": "2.0.0",
    "author": "EDD Customer Dashboard Pro",
    "homepage": "https://theweblab.xyz/",
    "screenshot": "screenshot.png",
    "tags": ["modern", "responsive", "glassmorphism", "alpine.js"],
    "supports": {
        "licenses": true,
        "wishlist": true,
        "analytics": true,
        "downloads": true,
        "purchases": true,
        "support": true,
        "fullscreen": true,
        "mobile": true,
        "dark_mode": true
    },
    "requires": {
        "php": "7.4",
        "wordpress": "6.0",
        "edd": "3.0"
    },
    "features": {
        "responsive": true,
        "accessible": true,
        "keyboard_navigation": true,
        "touch_friendly": true,
        "print_friendly": true,
        "rtl_ready": false
    },
    "assets": {
        "css": ["style.css"],
        "js": ["script.js"],
        "dependencies": {
            "alpinejs": "^3.0.0"
        }
    },
    "colors": {
        "primary": "#667eea",
        "secondary": "#764ba2",
        "success": "#10b981",
        "warning": "#f59e0b",
        "error": "#ef4444",
        "info": "#3b82f6"
    },
    "breakpoints": {
        "mobile": "480px",
        "tablet": "768px",
        "desktop": "1024px",
        "large": "1400px"
    },
    "customization": {
        "colors": true,
        "typography": true,
        "spacing": true,
        "animations": true
    },
    "performance": {
        "lazy_loading": true,
        "image_optimization": true,
        "code_splitting": false,
        "caching": true
    },
    "changelog": [
        {
            "version": "2.0.0",
            "date": "2025-01-29",
            "changes": [
                "Complete redesign with modern glassmorphism effects",
                "Replaced jQuery with Alpine.js for better performance",
                "Added comprehensive accessibility features",
                "Improved mobile responsiveness",
                "Added dark mode support",
                "Enhanced keyboard navigation",
                "Optimized for Core Web Vitals",
                "Removed backward compatibility code"
            ]
        },
        {
            "version": "1.0.0",
            "date": "2024-01-01",
            "changes": [
                "Initial release",
                "Basic dashboard functionality",
                "jQuery-based interactions"
            ]
        }
    ]
}